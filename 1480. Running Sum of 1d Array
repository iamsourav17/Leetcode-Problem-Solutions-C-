class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        vector <int> ans;                                  //new vector 
        int sum = 0;                                       //variable to add sum
        for(int i = 0; i<nums.size();i++)                  //loop through each element of nums
        {
            sum+= nums[i];                                 //add ith element to sum
            ans.push_back(sum);                            //include sum in new vector in each iterarion
        }
        
        return ans;                                         
        
    }
};
